{
  "name": "Chrome Notes",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A Demo App to implement a chrome-based notepad",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "View Notepad",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16" : "icon.png"
  }
}
